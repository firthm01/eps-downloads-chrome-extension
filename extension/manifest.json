{
  "manifest_version": 3,
  "name": "EPS Download Stats",
  "version": "1.0",
  "description": "Shows download stats for each version of the EAR Production Suite by platform.",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}